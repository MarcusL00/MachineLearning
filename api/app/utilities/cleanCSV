from turtle import pd


def cleanCSVData(csv_data_frame):
    csv_data_frame = pd.read_csv(csv_data_frame)
    
    DataCopy = csv_data_frame.copy()
    # calculate threshold for dropping columns with more than 30% missing values
    threshold = len(csv_data_frame) * 0.3
    # drop columns with more than 30% missing values
    DataCopy = DataCopy.dropna(axis=1, thresh=len(DataCopy) - threshold)