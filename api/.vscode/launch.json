{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Attach to .NET Core",
      "type": "coreclr",
      "request": "attach",
      "processName": "/app/bin/Debug/net9.0/CSVision",
      "sourceFileMap": {
        "/app": "${workspaceFolder}/CSVision"
      },
      "pipeTransport": {
        "pipeProgram": "docker",
        "pipeArgs": ["exec", "-i", "docker-csvision-api-1"],
        "debuggerPath": "/vsdbg/vsdbg",
        "pipeCwd": "${workspaceFolder}",
        "quoteArgs": false
      },
      "args": ["--interpreter=vscode"],
      "requireExactSource": false
    }
  ]
}
